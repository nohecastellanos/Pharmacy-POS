<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="331.0" prefWidth="563.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pos.pharmacy.signin.SigninCotroller">
    <children>
        <Button fx:id="btnlogin" layoutX="427.0" layoutY="227.0" onAction="#handleLoginAction" text="SignIn" />
        <Label fx:id="labellogin" layoutX="126" layoutY="120" minHeight="16" minWidth="69" />
      <TextField fx:id="txtusername" layoutX="230.0" layoutY="94.0" prefHeight="25.0" prefWidth="247.0" promptText="UserName" />
      <PasswordField fx:id="txtpassword" accessibleRole="PASSWORD_FIELD" layoutX="230.0" layoutY="146.0" prefHeight="25.0" prefWidth="247.0" promptText="Password" />
      <TextField id="txtpassword" fx:id="txtpasswordvisible" layoutX="230.0" layoutY="146.0" prefHeight="25.0" prefWidth="247.0" promptText="password" />
      <Label fx:id="txtwelcome" layoutY="-1.0" prefHeight="339.0" prefWidth="220.0" text="WELCOME" textFill="#0c0c0c" AnchorPane.leftAnchor="0.0">
         <font>
            <Font name="System Bold" size="31.0" />
         </font>
         <padding>
            <Insets left="25.0" />
         </padding>
      </Label>
      <CheckBox fx:id="showpasswordbox" layoutX="230.0" layoutY="180.0" mnemonicParsing="false" onAction="#handleHidePassword" prefHeight="17.0" prefWidth="144.0" text="Show Password" />
    </children>
</AnchorPane>
